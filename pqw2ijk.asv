function T = pqw2ijk(raan, inc, ap)

A = [cosd(raan), -sind(raan-180), 0;...
    sind(raan), cosd(raan-180), 0;...
    0, 0, 1];

B = [1, 0, 0;...
    0, cosd(inc), -sind(inc);...
    0, sind(inc), cosd(inc)];

C = [cosd(ap-180), -sind(ap-180), 0;...
    sind(ap-180), cosd(ap-180), 0;...
    0, 0, 1];

T = C*B*A;

end